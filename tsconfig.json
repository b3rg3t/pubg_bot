{
  "compilerOptions": {
  "target": "ES2022",
    "module": "CommonJS",
    "moduleResolution": "node",
    "strict": true,
    "noImplicitAny": true,
    "declaration": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "outDir": "./dist" /* Skip type checking all .d.ts files. */
  },
  "include": ["./src/**/*"],
  "exclude": ["node_modules"]
}
